<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

       <!-- Definition for textEditor bean -->

    <bean id ="consoleOut" class="com.render.ConsoleCalendarRender"></bean>
    <bean id ="HTMLOut" class="com.render.HTMLCalendarRender"></bean>

    <bean id ="CalendarApplication" class="com.Run.CalendarApplication" >
        <property name="printToConsole" ref="consoleOut" />
        <property name="printToHTML" ref="HTMLOut"/>
    </bean>>




</beans>